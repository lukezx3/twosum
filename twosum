#include <iostream>
using namespace std;


class Solution {
public:
    bool isValid(string s) {
        int count = 0;
        vector <char> vec;
        if(s.length() == 0){
            return true;
        }
        if(s.length() == 1){
            return false;
        }
        for(int i = 0; i < s.length(); i++){
            if(s[i] == '(' || s[i] == '[' || s[i] == '{'){
                vec.push_back(s[i]);
                count += 1;
            }
            else{
                if(count == 0){
                    return false;
                }
                else if(s[i] == ')'){
                    if(vec.at(vec.size()-1) == '('){
                        vec.pop_back();
                        count -=1;
                    }
                    else{
                        return false;
                    }
                }
                else if(s[i] == ']'){
                    if(vec.at(vec.size()-1) == '['){
                        vec.pop_back();
                        count -=1;
                    }
                    else{
                        return false;
                        
                    }
                }
                else if(s[i] == '}'){
                    if(vec.at(vec.size()-1) == '{'){
                        vec.pop_back();
                        count -=1;
                    }
                    else{
                        return false;
                    }
                }
            }
            //cout << vec.size() << ": COUNT" << endl;
            
        }
        if(vec.size() == 0){
            return true;
        }
        return false;
        
    }
};
